<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
  <appSettings>
    <add key="env:define" value="Development" xdt:Transform="InsertIfMissing" xdt:Locator="Match(key)" />
  </appSettings>
  <system.web>
    <compilation optimizeCompilations="true" xdt:Transform="SetAttributes(optimizeCompilations)">
    </compilation>
  </system.web>
</configuration>